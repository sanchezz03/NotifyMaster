{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TelegramBot": {
    "Token": "EnvironmentVariable:TELEGRAM_BOT_TOKEN"
  },
  "Hangfire": {
    "ConnectionString": "EnvironmentVariable:HANGFIRE_CONNECTION_STRING"
  },
  "Database": {
    "ConnectionString": "EnvironmentVariable:DATABASE_CONNECTION_STRING"
  }
}
